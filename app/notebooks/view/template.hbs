<div class="col-md-12">
	{{model.noteBook.title}}
</div>
<div>
<div class="col-md-4">
	<div>
	Title: {{input value=title}} <button {{action 'addNote'}}>Add</button>
	</div>
	<div>
		<ul>
		{{#each model.notes as |note|}}
			<li>
			{{#link-to 'notebooks.view.notes' note}}
			{{note.title}}
			{{/link-to}}
			<button {{action 'deleteNote' note}}>Delete</button>
			</li>
		{{else}}
			<div>No notes found</div>
		{{/each}}
		</ul>
	</div>
</div>
	<div class="col-md-8">
	{{outlet}}
	</div>
</div>