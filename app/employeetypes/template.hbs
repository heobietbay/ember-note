<div>
	<div class="col-md-6">
		<h3>Employee Type Maintenance</h3>
	<div>
		<strong>Name: </strong>{{input value=name}}
		<button {{action 'add'}}>Add</button>
	</div>
	<p>
		<h4>Current employee types:</h4>
		<ul>
		{{#each model as |employeeType|}}		
			<li>{{employeeType.name}}</li>
		{{else}}
			<div>No types found</div>
		{{/each}}
		</ul>
	</p>
	</div>
</div>